{
  "name": "angular2-clipboard",
  "description": "angular 2 clipboard",
  "main": "index.js",
  "author": {
    "name": "Sam Lin",
    "email": "maxisam@gmail.com"
  },
  "homepage": "https://github.com/maxisam/angular2-clipboard",
  "scripts": {
    "tslint": "tslint -c tslint.json 'src/**/*.ts'",
    "commit": "git-cz",
    "publish-npm": "npm publish",
    "build": "npm-run-all --parallel build:*",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "dev:build": "webpack --output-filename index.umd.js --progress --profile --bail",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "engines": {
    "node": ">= 4.2.1"
  },
  "license": "MIT",
  "publishConfig": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/maxisam/angular2-clipboard"
  },
  "keywords": [
    "angular",
    "clipboard",
    "copy"
  ],
  "dependencies": {
    "@angular/core": "^2.0.0",
    "@types/clipboard": "1.5.29",
    "clipboard": "~1.5.12",
    "core-js": "^2.4.1",
    "rxjs": "~5.0.0-beta.12",
    "zone.js": "~0.6.25"
  },
  "devDependencies": {
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "es6-promise": "^3.1.2",
    "es6-promise-loader": "^1.0.1",
    "es6-shim": "^0.35.0",
    "es7-reflect-metadata": "^1.6.0",
    "npm-run-all": "^3.1.0",
    "semantic-release": "^4.3.5",
    "ts-loader": "^0.8.2",
    "ts-node": "^1.2.2",
    "typescript": "^2.0.3",
    "typings": "^1.3.3",
    "webpack": "^1.13.0",
    "webpack-merge": "^0.14.1"
  },
  "files": [
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json",
    "index.*",
    "src/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "version": "1.0.2"
}